{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport { reactive } from 'vue';\nimport { post } from '../../utils/request.js';\nimport Toast from '../../components/Toast.vue';\nexport default {\n  name: 'Login',\n  components: {\n    Toast\n  },\n\n  setup() {\n    const data = reactive({\n      username: '',\n      password: '',\n      showToast: false,\n      toastMessage: ''\n    }); // 路由权限跳转 islogin\n\n    const router = useRouter();\n\n    const handlelogin = async () => {\n      try {\n        const result = await post('/1vue/login', {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: 'Home'\n          });\n        } else {\n          data.showToast = true;\n          data.toastMessage = '登录失败';\n        }\n      } catch (e) {\n        alert('请求失败');\n      }\n    };\n\n    const handleRegisterClcik = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handlelogin,\n      handleRegisterClcik,\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,SAASC,IAAT,QAAqB,wBAArB;AACA,OAAOC,KAAP,MAAkB,4BAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,OAAI,GAAK;AACP,UAAMC,IAAG,GAAIN,QAAQ,CAAC;AACpBO,cAAQ,EAAE,EADU;AAEpBC,cAAQ,EAAE,EAFU;AAGpBC,eAAS,EAAE,KAHS;AAIpBC,kBAAY,EAAE;AAJM,KAAD,CAArB,CADO,CAOP;;AACA,UAAMC,MAAK,GAAIb,SAAS,EAAxB;;AACA,UAAMc,WAAU,GAAI,YAAY;AAC9B,UAAI;AACF,cAAMC,MAAK,GAAI,MAAMZ,IAAI,CAAC,aAAD,EAAgB;AACvCM,kBAAQ,EAAED,IAAI,CAACC,QADwB;AAEvCC,kBAAQ,EAAEF,IAAI,CAACE;AAFwB,SAAhB,CAAzB;;AAIA,YAAIK,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;AACvBC,sBAAY,CAACC,OAAb,GAAuB,IAAvB;AACAL,gBAAM,CAACM,IAAP,CAAY;AAAEd,gBAAI,EAAE;AAAR,WAAZ;AACF,SAHA,MAGO;AACLG,cAAI,CAACG,SAAL,GAAiB,IAAjB;AACAH,cAAI,CAACI,YAAL,GAAoB,MAApB;AACF;AACF,OAZA,CAYE,OAAOQ,CAAP,EAAU;AACVC,aAAK,CAAC,MAAD,CAAL;AACF;AACF,KAhBA;;AAiBA,UAAMC,mBAAkB,GAAI,MAAM;AAChCT,YAAM,CAACM,IAAP,CAAY;AAAEd,YAAI,EAAE;AAAR,OAAZ;AACF,KAFA;;AAGA,WAAO;AAAES,iBAAF;AAAeQ,yBAAf;AAAoCd;AAApC,KAAP;AACF;;AAnCa,CAAf","names":["useRouter","axios","reactive","post","Toast","name","components","setup","data","username","password","showToast","toastMessage","router","handlelogin","result","error","localStorage","isLogin","push","e","alert","handleRegisterClcik"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/login/Login.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\">\n    <div class=\"wrapper__input\">\n      <input class=\"wrapper__input__content\" v-model=\"data.username\" placeholder=\"请输入用户名\"/>\n    </div>\n    <div class=\"wrapper__input\">\n      <input type=\"password\" class=\"wrapper__input__content\"  v-model=\"data.password\" placeholder=\"请输入密码\" />\n    </div>\n    <div class=\"wrapper__login-button\" @click='handlelogin'>登录</div>\n    <div class=\"wrapper__login-link\">\n      <span class=\"wrapper__login-link-registe\" @click='handleRegisterClcik'>立即注册</span>\n      <span class=\"wrapper__login-link-separate\">|</span>\n      <span class=\"wrapper__login-link-reset\">忘记密码</span>\n    </div>\n  </div>\n  <Toast v-if='data.showToast' message='data.toa' />\n</template>\n\n<script>\nimport { useRouter } from 'vue-router'\nimport axios from 'axios'\nimport { reactive } from 'vue'\nimport { post } from '../../utils/request.js'\nimport Toast from '../../components/Toast.vue'\n\nexport default {\n  name: 'Login',\n  components: {\n    Toast\n  },\n  setup () {\n    const data = reactive({\n      username: '',\n      password: '',\n      showToast: false,\n      toastMessage: ''\n    })\n    // 路由权限跳转 islogin\n    const router = useRouter()\n    const handlelogin = async () => {\n      try {\n        const result = await post('/1vue/login', {\n          username: data.username,\n          password: data.password\n        })\n        if (result?.error === 0) {\n          localStorage.isLogin = true\n          router.push({ name: 'Home' })\n        } else {\n          data.showToast = true\n          data.toastMessage = '登录失败'\n        }\n      } catch (e) {\n        alert('请求失败')\n      }\n    }\n    const handleRegisterClcik = () => {\n      router.push({ name: 'Register' })\n    }\n    return { handlelogin, handleRegisterClcik, data }\n  }\n}\n</script>\n\n<style lang='scss' scoped>\n@import '../../style/viriables.scss';\n  .wrapper {\n    position: absolute;\n    top: 50%;\n    left: 0;\n    right: 0;\n    transform: translateY(-50%);\n    &__img {\n      display: block;\n      margin:0 auto .4rem;\n      width: .66rem;\n      height: .66rem\n    }\n    &__input {\n      margin: 0 .4rem .16rem;\n      padding: 0 .16rem;\n      height: .48rem;\n      background: #F9F9F9;\n      border: 1px solid rgba(0,0,0,0.10);\n      border-radius: .06rem;;\n      &__content {\n        width: 100%;\n        line-height: .48rem;\n        border: none;\n        outline: none;\n        font-size: .16rem;\n        background: none;\n        &::placeholder {\n          color: $content-notice-fontcolor;\n        }\n      }\n    }\n    &__login-button {\n      margin: .32rem .4rem .16rem;\n      height: .48rem;\n      box-shadow: 0 .04rem .08rem 0 rgba(0,145,255,0.32);\n      border-radius: .04rem;\n      background: #0091FF;\n      text-align: center;\n      line-height: .48rem;\n      font-size: .16rem;\n      color: #fff;\n    }\n    &__login-link {\n      text-align: center;\n      font-size: .14rem;\n      color: $content-notice-fontcolor;\n      &-separate {\n        margin: 0 .12rem;\n      }\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}