{"ast":null,"code":"import { useRoute, useRouter } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects.js';\nimport TopArea from './TopArea.vue';\nimport ProductList from '/';\nexport default {\n  name: 'orderConfirmation',\n  components: {\n    TopArea\n  },\n\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const shopId = route.params.id;\n    const {\n      productList,\n      shopName,\n      calculations\n    } = useCommonCartEffect(shopId);\n\n    const handleClickBack = () => {\n      router.back();\n    };\n\n    return {\n      productList,\n      shopName,\n      calculations,\n      handleClickBack\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,YAApC;AACA,SAASC,mBAAT,QAAoC,8BAApC;AACA,OAAOC,OAAP,MAAoB,eAApB;AACA,OAAOC,WAAP,MAAwB,GAAxB;AAEA,eAAe;AACbC,MAAI,EAAE,mBADO;AAEbC,YAAU,EAAE;AACVH;AADU,GAFC;;AAKbI,OAAI,GAAK;AACP,UAAMC,KAAI,GAAIR,QAAQ,EAAtB;AACA,UAAMS,MAAK,GAAIR,SAAS,EAAxB;AACA,UAAMS,MAAK,GAAIF,KAAK,CAACG,MAAN,CAAaC,EAA5B;AACA,UAAM;AAAEC,iBAAF;AAAeC,cAAf;AAAyBC;AAAzB,QAA0Cb,mBAAmB,CAACQ,MAAD,CAAnE;;AACA,UAAMM,eAAc,GAAI,MAAM;AAAEP,YAAM,CAACQ,IAAP;AAAc,KAA9C;;AACA,WAAO;AAAEJ,iBAAF;AAAeC,cAAf;AAAyBC,kBAAzB;AAAuCC;AAAvC,KAAP;AACF;;AAZa,CAAf","names":["useRoute","useRouter","useCommonCartEffect","TopArea","ProductList","name","components","setup","route","router","shopId","params","id","productList","shopName","calculations","handleClickBack","back"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/orderConfirmation/OrderConfirmation.vue"],"sourcesContent":["<template>\n  <div class=\"wraper\">\n    <TopArea />\n    \n    <div class=\"order\">\n      <div class=\"order__price\">实付金额\n        <span class=\"order__price__money\">&yen;{{calculations.price}}</span>\n      </div>\n      <div class=\"order__btn\">提交订单</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRoute, useRouter } from 'vue-router'\nimport { useCommonCartEffect } from '../../effects/cartEffects.js'\nimport TopArea from './TopArea.vue'\nimport ProductList from '/'\n\nexport default {\n  name: 'orderConfirmation',\n  components: {\n    TopArea\n  },\n  setup () {\n    const route = useRoute()\n    const router = useRouter()\n    const shopId = route.params.id\n    const { productList, shopName, calculations } = useCommonCartEffect(shopId)\n    const handleClickBack = () => { router.back() }\n    return { productList, shopName, calculations, handleClickBack }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import '../../style/viriables.scss';\n@import '../../style/mixins.scss';\n\n  .wraper {\n    overflow-y: auto;\n    position: absolute;\n    padding: 0 .18rem;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background-color: #eee;\n    margin-bottom: .55rem;\n  }\n \n\n  // 底部\n  .order {\n    position: fixed;\n    display: flex;\n    width: 100%;\n    align-items: center;\n    justify-content: space-between;\n    padding-left: .18rem;\n    box-sizing: border-box;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    height: .49rem;\n    line-height: .49rem;\n    background-color: #fff;\n    &__price {\n      font-size: 14px;\n      color: #333333;\n      font-size: .14rem;\n      line-height: .20rem;\n      &__money {\n        font-size: .16rem;\n        line-height: .22rem;\n        font-weight: bold;\n      }\n    }\n    &__btn {\n      width: .98rem;\n      background-color: #4FB0F9;\n      text-align: center;\n      font-size: .14rem;\n      color: #fff;\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}