{"ast":null,"code":"import { reactive, tor } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { post } from '../../utils/request.js';\n\nconst useRegisterEffect = errMessage => {\n  const router = useRouter();\n  const data = reactive({\n    username: '',\n    password: '',\n    checkPassword: ''\n  });\n\n  const handleregister = async () => {\n    try {\n      const result = await post('//vue/register', {\n        username: data.username,\n        password: data.password,\n        checkPassword: data.checkPassword\n      });\n\n      if (result?.error === 0) {\n        localStorage.isRegister = true;\n        router.push({\n          name: 'Login'\n        });\n      } else {\n        errMessage('注册失败');\n      }\n    } catch (e) {\n      errMessage('请求失败');\n    }\n  };\n};\n\nexport default {\n  name: 'Register',\n\n  setup() {\n    const router = useRouter();\n\n    const handleLoginClick = () => {\n      router.push({\n        name: 'Login'\n      });\n    };\n\n    return {\n      handleLoginClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAASC,IAAT,QAAqB,wBAArB;;AAEA,MAAMC,iBAAgB,GAAKC,UAAD,IAAgB;AACxC,QAAMC,MAAK,GAAIJ,SAAS,EAAxB;AACA,QAAMK,IAAG,GAAIP,QAAQ,CAAC;AACpBQ,YAAQ,EAAE,EADU;AAEpBC,YAAQ,EAAE,EAFU;AAGpBC,iBAAa,EAAE;AAHK,GAAD,CAArB;;AAKA,QAAMC,cAAa,GAAI,YAAY;AACjC,QAAI;AACF,YAAMC,MAAK,GAAI,MAAMT,IAAI,CAAC,gBAAD,EAAmB;AAC1CK,gBAAQ,EAAED,IAAI,CAACC,QAD2B;AAE1CC,gBAAQ,EAAEF,IAAI,CAACE,QAF2B;AAG1CC,qBAAa,EAAEH,IAAI,CAACG;AAHsB,OAAnB,CAAzB;;AAKA,UAAIE,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;AACvBC,oBAAY,CAACC,UAAb,GAA0B,IAA1B;AACAT,cAAM,CAACU,IAAP,CAAY;AAAEC,cAAI,EAAE;AAAR,SAAZ;AACF,OAHA,MAGO;AACLZ,kBAAU,CAAC,MAAD,CAAV;AACF;AACF,KAZA,CAYE,OAAOa,CAAP,EAAU;AACVb,gBAAU,CAAC,MAAD,CAAV;AACF;AACF,GAhBA;AAkBF,CAzBA;;AA2BA,eAAe;AACbY,MAAI,EAAE,UADO;;AAEbE,OAAI,GAAK;AACP,UAAMb,MAAK,GAAIJ,SAAS,EAAxB;;AACA,UAAMkB,gBAAe,GAAI,MAAM;AAC7Bd,YAAM,CAACU,IAAP,CAAY;AAAEC,YAAI,EAAE;AAAR,OA