{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects.js';\nexport default {\n  name: 'orderConfirmation',\n\n  setup() {\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      productList,\n      shopName,\n      calculations\n    } = useCommonCartEffect(shopId);\n    return {\n      productList,\n      shopName,\n      calculations\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwCA,SAASA,QAAT,QAAyB,YAAzB;AACA,SAASC,mBAAT,QAAoC,8BAApC;AACA,eAAe;AACbC,MAAI,EAAE,mBADO;;AAEbC,OAAI,GAAK;AACP,UAAMC,KAAI,GAAIJ,QAAQ,EAAtB;AACA,UAAMK,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;AACA,UAAM;AAAEC,iBAAF;AAAeC,cAAf;AAAyBC;AAAzB,QAA0CT,mBAAmB,CAACI,MAAD,CAAnE;AACA,WAAO;AAAEG,iBAAF;AAAeC,cAAf;AAAyBC;AAAzB,KAAP;AACF;;AAPa,CAAf","names":["useRoute","useCommonCartEffect","name","setup","route","shopId","params","id","productList","shopName","calculations"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/orderConfirmation/OrderConfirmation.vue"],"sourcesContent":["<template>\n  <div class=\"wraper\">\n    <div class=\"top\">\n      <div class=\"top__header\">\n        <div class=\"iconfont top__header__back\">&#xe6db;</div>\n        确认订单\n      </div>\n      <div class=\"top__receiver\">\n        <div class=\"top__receiver__title\">收货地址</div>\n        <div class=\"top__receiver__address\">北京理工大学国防科技园2号楼10层</div>\n        <div class=\"top__receiver__info\">\n          <span class=\"top__receiver__info__name\">瑶妹（先生）</span>\n          <span class=\"top__receiver__info__name\">18539150233</span>\n        </div>\n        <div class=\"iconfont top__receiver__enter\">&#xe6db;</div>\n      </div>\n    </div>\n    <div class=\"products\">\n      <div class=\"products__title\">{{shopName}}</div>\n      <div class=\"products__list\">\n        <div class=\"products__item\" v-for=\"item in productList\" :key='item._id'>\n          <img class=\"products__item__img\" :src=\"item.imgUrl\">\n          <div class=\"products__item__detail\">\n            <h4 class=\"products__item__title\">{{item.name}}</h4>\n            <p class=\"products__item__price\">\n              <span class=\"products__item__yen\">&yen;{{item.price}} * {{item.count}}</span>\n              <span class=\"products__item__origin\">&yen;{{item.price * item.count}}</span>\n            </p>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"order\">\n      <div class=\"order__price\">实付金额&yen;{{calculations.price}}</div>\n      <div class=\"order__btn\">提交订单</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRoute } from 'vue-router'\nimport { useCommonCartEffect } from '../../effects/cartEffects.js'\nexport default {\n  name: 'orderConfirmation',\n  setup () {\n    const route = useRoute()\n    const shopId = route.params.id\n    const { productList, shopName, calculations } = useCommonCartEffect(shopId)\n    return { productList, shopName, calculations }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import '../../style/viriables.scss';\n@import '../../style/mixins.scss';\n\n  .wraper {\n    position: absolute;\n    padding: 0 .18rem;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background-color: #eee;\n  }\n  .top {\n    position: relative;\n    padding: 0 .18rem;\n    margin: 0 -.18rem;\n    height: 1.96rem;\n    background-size: 100% 1.59rem;\n    background-image: linear-gradient(0deg, rgba(0,145,255,0.00) 4%, #0091FF 50%);\n    background-repeat: no-repeat;\n    &__header {\n      position: relative;\n      padding-top: .26rem;\n      line-height: .24rem;\n      text-align: center;\n      font-size: .16rem;\n      color: #fff;\n      &__back {\n        position: absolute;\n        left: 0;\n        line-height: .22rem;\n        font-size: .22rem;\n      }\n    }\n    &__receiver {\n      position: absolute;\n      padding: .16rem .16rem;\n      left: .18rem;\n      right: .18rem;\n      bottom: 0;\n      height: 1.11rem;\n      box-sizing: border-box;\n      background-color: #fff;\n      border-radius: .04rem;\n      &__title {\n        line-height: .22rem;\n        font-size: .16rem;\n        padding-bottom: .14rem;\n        color: #333;\n      }\n      &__address {\n        line-height: .2rem;\n        font-size: .14rem;\n        padding-bottom: .06rem;\n        color: #333;\n      }\n      &__info__name {\n        font-size: .14rem;\n        color: #666;\n      }\n      &__enter {\n        transform: translateY(-50%) rotate(180deg);\n        position: absolute;\n        top: 50%;\n        right: .16rem;\n        // margin-top: -.08rem;\n        // width: .16rem;\n        // height: .16rem;\n        font-size: .22rem;\n        // transform: translateY(-50%);\n      }\n    }\n  }\n  // 商品信息布局\n  .products {\n    margin-top: .16rem;\n    background-color: #fff;\n    &__title {\n      padding: .16rem 0 .16rem .16rem;\n      font-size: .16rem;\n      color: #333;\n    }\n    &__list {\n     padding: 0 .16rem;\n    }\n    &__item {\n      position: relative;\n      display: flex;\n      padding: 0 0 .16rem 0;\n      &__img {\n        margin: 0 .16rem 0 0;\n        width: .46rem;\n        height: .46rem;\n      }\n      &__detail {\n        // overflow: hidden;\n        // width: 100%;\n        flex: 1;\n      }\n      &__title {\n        margin-bottom: .06rem;\n        font-weight: normal;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $content-fontcolor;\n        @include ellipsis;\n      }\n      &__price {\n        display: flex;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $highlight-color;\n      }\n      &__origin {\n        flex: 1;\n        text-align: right;\n        color: #000;\n      }\n    }\n  }\n  // 底部\n  .order {\n    position: fixed;\n    display: flex;\n    width: 100%;\n    align-items: center;\n    justify-content: space-between;\n    margin: 0 -.18rem;\n    padding: 0 .18rem;\n    bottom: 0;\n    height: .5rem;\n    background-color: #fff;\n    &\n    &__btn {\n      flex: 1;\n      // text-align: right;\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}