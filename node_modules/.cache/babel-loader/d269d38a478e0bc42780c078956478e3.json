{"ast":null,"code":"import { reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport { post } from '../../utils/request.js'; // 引入蒙层函数\n\nimport Toast, { useToastEffect } from '../../components/Toast.vue';\n\nconst useLoginEffect = () => {\n  const data = reactive({\n    username: '',\n    password: ''\n  });\n};\n\nexport default {\n  name: 'Login',\n  components: {\n    Toast\n  },\n\n  setup() {\n    // 解构蒙层函数代码\n    const {\n      toastData,\n      errMessage\n    } = useToastEffect(); // 路由权限跳转 islogin\n\n    const router = useRouter();\n\n    const handlelogin = async () => {\n      try {\n        const result = await post('/1vue/login', {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: 'Home'\n          });\n        } else {\n          errMessage('登录失败');\n        }\n      } catch (e) {\n        errMessage('请求失败');\n      }\n    };\n\n    const handleRegisterClcik = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handlelogin,\n      handleRegisterClcik,\n      data,\n      toastData\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,IAAT,QAAqB,wBAArB,C,CACA;;AACA,OAAOC,KAAP,IAAgBC,cAAhB,QAAsC,4BAAtC;;AAEA,MAAMC,cAAa,GAAI,MAAM;AAC1B,QAAMC,IAAG,GAAIP,QAAQ,CAAC;AACnBQ,YAAQ,EAAE,EADS;AAEnBC,YAAQ,EAAE;AAFS,GAAD,CAArB;AAIH,CALA;;AAOA,eAAe;AACbC,MAAI,EAAE,OADO;AAEbC,YAAU,EAAE;AACVP;AADU,GAFC;;AAKbQ,OAAI,GAAK;AACP;AACA,UAAM;AAAEC,eAAF;AAAaC;AAAb,QAA4BT,cAAc,EAAhD,CAFO,CAIP;;AACA,UAAMU,MAAK,GAAId,SAAS,EAAxB;;AACA,UAAMe,WAAU,GAAI,YAAY;AAC9B,UAAI;AACF,cAAMC,MAAK,GAAI,MAAMd,IAAI,CAAC,aAAD,EAAgB;AACvCK,kBAAQ,EAAED,IAAI,CAACC,QADwB;AAEvCC,kBAAQ,EAAEF,IAAI,CAACE;AAFwB,SAAhB,CAAzB;;AAIA,YAAIQ,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;AACvBC,s