{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects.js';\nexport default {\n  name: 'ProductList',\n\n  setup() {\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      productList,\n      shopName\n    } = useCommonCartEffect(shopId);\n    return {\n      productList,\n      shopName\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,QAAT,QAAyB,YAAzB;AACA,SAASC,mBAAT,QAAoC,8BAApC;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;;AAEbC,OAAI,GAAK;AACP,UAAMC,KAAI,GAAIJ,QAAQ,EAAtB;AACA,UAAMK,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;AACA,UAAM;AAAEC,iBAAF;AAAeC;AAAf,QAA4BR,mBAAmB,CAACI,MAAD,CAArD;AACA,WAAO;AAAEG,iBAAF;AAAeC;AAAf,KAAP;AACF;;AAPa,CAAf","names":["useRoute","useCommonCartEffect","name","setup","route","shopId","params","id","productList","shopName"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/orderConfirmation/ProductList.vue"],"sourcesContent":["<template>\n  <div class=\"products\">\n    <div class=\"products__title\">{{shopName}}</div>\n    <div class=\"products__list\">\n      <div class=\"products__item\" v-for=\"item in productList\" :key='item._id'>\n        <img class=\"products__item__img\" :src=\"item.imgUrl\">\n        <div class=\"products__item__detail\">\n          <h4 class=\"products__item__title\">{{item.name}}</h4>\n          <p class=\"products__item__price\">\n            <span class=\"products__item__yen\">&yen;{{item.price}} * {{item.count}}</span>\n            <span class=\"products__item__origin\">&yen;{{item.price * item.count}}</span>\n          </p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRoute } from 'vue-router'\nimport { useCommonCartEffect } from '../../effects/cartEffects.js'\n\nexport default {\n  name: 'ProductList',\n  setup () {\n    const route = useRoute()\n    const shopId = route.params.id\n    const { productList, shopName } = useCommonCartEffect(shopId)\n    return { productList, shopName }\n  }\n}\n</script>\n\n<style lang='scss' scoped>\n@import '../../style/viriables.scss';\n@import '../../style/mixins.scss';\n\n  // 商品信息布局\n  .products {\n    margin-top: .16rem;\n    background-color: $bgColor;\n    &__title {\n      padding: .16rem 0 .16rem .16rem;\n      font-size: .16rem;\n      color: $content-fontcolor;\n    }\n    &__list {\n      padding: 0 .16rem;\n    }\n    &__item {\n      position: relative;\n      display: flex;\n      padding: 0 0 .16rem 0;\n      &__img {\n        margin: 0 .16rem 0 0;\n        width: .46rem;\n        height: .46rem;\n      }\n      &__detail {\n        // overflow: hidden;\n        // width: 100%;\n        flex: 1;\n      }\n      &__title {\n        margin-bottom: .06rem;\n        font-weight: normal;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $content-fontcolor;\n        @include ellipsis;\n      }\n      &__price {\n        display: flex;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $highlight-color;\n      }\n      &__origin {\n        flex: 1;\n        text-align: right;\n        color: #000;\n      }\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}