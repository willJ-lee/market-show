{"ast":null,"code":"import { reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport { post } from '../../utils/request.js';\nimport Toast, { useToastEffect } from '../../components/Toast.vue';\nexport default {\n  name: 'Login',\n  components: {\n    Toast\n  },\n\n  setup() {\n    const data = reactive({\n      username: '',\n      password: ''\n    }); // 路由权限跳转 islogin\n\n    const router = useRouter();\n\n    const handlelogin = async () => {\n      try {\n        const result = await post('/1vue/login', {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result?.error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: 'Home'\n          });\n        } else {\n          errMessage('登录失败');\n        }\n      } catch (e) {\n        errMessage('请求失败');\n      }\n    };\n\n    const handleRegisterClcik = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handlelogin,\n      handleRegisterClcik,\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,QAAT,QAAyB,KAAzB;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,IAAT,QAAqB,wBAArB;AACA,OAAOC,KAAP,IAAgBC,cAAhB,QAAsC,4BAAtC;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;AAEbC,YAAU,EAAE;AACVH;AADU,GAFC;;AAKbI,OAAI,GAAK;AACP,UAAMC,IAAG,GAAIT,QAAQ,CAAC;AACpBU,cAAQ,EAAE,EADU;AAEpBC,cAAQ,EAAE;AAFU,KAAD,CAArB,CADO,CAMP;;AACA,UAAMC,MAAK,GAAIX,SAAS,EAAxB;;AACA,UAAMY,WAAU,GAAI,YAAY;AAC9B,UAAI;AACF,cAAMC,MAAK,GAAI,MAAMX,IAAI,CAAC,aAAD,EAAgB;AACvCO,kBAAQ,EAAED,IAAI,CAACC,QADwB;AAEvCC,kBAAQ,EAAEF,IAAI,CAACE;AAFwB,SAAhB,CAAzB;;AAIA,YAAIG,MAAM,EAAEC,KAAR,KAAkB,CAAtB,EAAyB;AACvBC,sBAAY,CAACC,OAAb,GAAuB,IAAvB;AACAL,gBAAM,CAACM,IAAP,CAAY;AAAEZ,gBAAI,EAAE;AAAR,WAAZ;AACF,SAHA,MAGO;AACLa,oBAAU,CAAC,MAAD,CAAV;AACF;AACF,OAXA,CAWE,OAAOC,CAAP,EAAU;AACVD,kBAAU,CAAC,MAAD,CAAV;AACF;AACF,KAfA;;AAgBA,UAAME,mBAAkB,GAAI,MAAM;AAChCT,YAAM,CAACM,IAAP,C