{"ast":null,"code":"import { useRoute, useRouter } from 'vue-router';\nimport { useCommonCartEffect } from '../../effects/cartEffects.js';\nimport TopArea from './';\nexport default {\n  name: 'orderConfirmation',\n\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const shopId = route.params.id;\n    const {\n      productList,\n      shopName,\n      calculations\n    } = useCommonCartEffect(shopId);\n\n    const handleClickBack = () => {\n      router.back();\n    };\n\n    return {\n      productList,\n      shopName,\n      calculations,\n      handleClickBack\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,YAApC;AACA,SAASC,mBAAT,QAAoC,8BAApC;AACA,OAAOC,OAAP,MAAoB,IAApB;AAEA,eAAe;AACbC,MAAI,EAAE,mBADO;;AAEbC,OAAI,GAAK;AACP,UAAMC,KAAI,GAAIN,QAAQ,EAAtB;AACA,UAAMO,MAAK,GAAIN,SAAS,EAAxB;AACA,UAAMO,MAAK,GAAIF,KAAK,CAACG,MAAN,CAAaC,EAA5B;AACA,UAAM;AAAEC,iBAAF;AAAeC,cAAf;AAAyBC;AAAzB,QAA0CX,mBAAmB,CAACM,MAAD,CAAnE;;AACA,UAAMM,eAAc,GAAI,MAAM;AAAEP,YAAM,CAACQ,IAAP;AAAc,KAA9C;;AACA,WAAO;AAAEJ,iBAAF;AAAeC,cAAf;AAAyBC,kBAAzB;AAAuCC;AAAvC,KAAP;AACF;;AATa,CAAf","names":["useRoute","useRouter","useCommonCartEffect","TopArea","name","setup","route","router","shopId","params","id","productList","shopName","calculations","handleClickBack","back"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/orderConfirmation/OrderConfirmation.vue"],"sourcesContent":["<template>\n  <div class=\"wraper\">\n    \n    <div class=\"products\">\n      <div class=\"products__title\">{{shopName}}</div>\n      <div class=\"products__list\">\n        <div class=\"products__item\" v-for=\"item in productList\" :key='item._id'>\n          <img class=\"products__item__img\" :src=\"item.imgUrl\">\n          <div class=\"products__item__detail\">\n            <h4 class=\"products__item__title\">{{item.name}}</h4>\n            <p class=\"products__item__price\">\n              <span class=\"products__item__yen\">&yen;{{item.price}} * {{item.count}}</span>\n              <span class=\"products__item__origin\">&yen;{{item.price * item.count}}</span>\n            </p>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"order\">\n      <div class=\"order__price\">实付金额\n        <span class=\"order__price__money\">&yen;{{calculations.price}}</span>\n      </div>\n      <div class=\"order__btn\">提交订单</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRoute, useRouter } from 'vue-router'\nimport { useCommonCartEffect } from '../../effects/cartEffects.js'\nimport TopArea from './'\n\nexport default {\n  name: 'orderConfirmation',\n  setup () {\n    const route = useRoute()\n    const router = useRouter()\n    const shopId = route.params.id\n    const { productList, shopName, calculations } = useCommonCartEffect(shopId)\n    const handleClickBack = () => { router.back() }\n    return { productList, shopName, calculations, handleClickBack }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import '../../style/viriables.scss';\n@import '../../style/mixins.scss';\n\n  .wraper {\n    overflow-y: auto;\n    position: absolute;\n    padding: 0 .18rem;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background-color: #eee;\n    margin-bottom: .55rem;\n  }\n \n  // 商品信息布局\n  .products {\n    margin-top: .16rem;\n    background-color: #fff;\n    &__title {\n      padding: .16rem 0 .16rem .16rem;\n      font-size: .16rem;\n      color: #333;\n    }\n    &__list {\n      padding: 0 .16rem;\n    }\n    &__item {\n      position: relative;\n      display: flex;\n      padding: 0 0 .16rem 0;\n      &__img {\n        margin: 0 .16rem 0 0;\n        width: .46rem;\n        height: .46rem;\n      }\n      &__detail {\n        // overflow: hidden;\n        // width: 100%;\n        flex: 1;\n      }\n      &__title {\n        margin-bottom: .06rem;\n        font-weight: normal;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $content-fontcolor;\n        @include ellipsis;\n      }\n      &__price {\n        display: flex;\n        line-height: .2rem;\n        font-size: .14rem;\n        color: $highlight-color;\n      }\n      &__origin {\n        flex: 1;\n        text-align: right;\n        color: #000;\n      }\n    }\n  }\n  // 底部\n  .order {\n    position: fixed;\n    display: flex;\n    width: 100%;\n    align-items: center;\n    justify-content: space-between;\n    padding-left: .18rem;\n    box-sizing: border-box;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    height: .49rem;\n    line-height: .49rem;\n    background-color: #fff;\n    &__price {\n      font-size: 14px;\n      color: #333333;\n      font-size: .14rem;\n      line-height: .20rem;\n      &__money {\n        font-size: .16rem;\n        line-height: .22rem;\n        font-weight: bold;\n      }\n    }\n    &__btn {\n      width: .98rem;\n      background-color: #4FB0F9;\n      text-align: center;\n      font-size: .14rem;\n      color: #fff;\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}