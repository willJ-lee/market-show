{"ast":null,"code":"import { computed, defineCustomElement } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'Cart',\n\n  setup() {\n    const store = useStore();\n    const route = useRoute();\n    const shopId = route.params.id;\n    const cartList = store.state.cartList; // 计算购物车物品数量\n\n    const cartTotal = computed(() => {\n      const productList = cartList[shopId];\n      let count = 0;\n\n      if (productList) {\n        for (const i in productList) {\n          const produnct = productList[i];\n          count += produnct.count;\n        }\n      }\n\n      return count;\n    }); // 计算购物车物品价格\n\n    const cartPrice = computed(() => {\n      const productList = cartList[shopId];\n      let price = 0;\n\n      if (productList) {\n        for (const i in productList) {\n          const product = productList[i];\n          price = product.count * product.price;\n        }\n      }\n    });\n    return {\n      cartTotal\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,SAASA,QAAT,EAAmBC,mBAAnB,QAA8C,KAA9C;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACbC,MAAI,EAAE,MADO;;AAEbC,OAAI,GAAK;AACP,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,KAAI,GAAIL,QAAQ,EAAtB;AACA,UAAMM,MAAK,GAAID,KAAK,CAACE,MAAN,CAAaC,EAA5B;AACA,UAAMC,QAAO,GAAIL,KAAK,CAACM,KAAN,CAAYD,QAA7B,CAJO,CAKP;;AACA,UAAME,SAAQ,GAAIb,QAAQ,CAAC,MAAM;AAC/B,YAAMc,WAAU,GAAIH,QAAQ,CAACH,MAAD,CAA5B;AACA,UAAIO,KAAI,GAAI,CAAZ;;AACA,UAAID,WAAJ,EAAiB;AACf,aAAK,MAAME,CAAX,IAAgBF,WAAhB,EAA6B;AAC3B,gBAAMG,QAAO,GAAIH,WAAW,CAACE,CAAD,CAA5B;AACAD,eAAI,IAAKE,QAAQ,CAACF,KAAlB;AACF;AACF;;AACA,aAAOA,KAAP;AACD,KAVyB,CAA1B,CANO,CAiBP;;AACA,UAAMG,SAAQ,GAAIlB,QAAQ,CAAC,MAAM;AAC/B,YAAMc,WAAU,GAAIH,QAAQ,CAACH,MAAD,CAA5B;AACA,UAAIW,KAAI,GAAI,CAAZ;;AACA,UAAIL,WAAJ,EAAiB;AACf,aAAK,MAAME,CAAX,IAAgBF,WAAhB,EAA6B;AAC3B,gBAAMM,OAAM,GAAIN,WAAW,CAACE,CAAD,CAA3B;AACAG,eAAI,GAAIC,OAAO,CAACL,KAAR,GAAgBK,OAAO,CAACD,KAAhC;AACF;AACF;AACD,KATyB,CAA1B;AAUA,WAAO;AAAEN;AAAF,KAAP;AACF;;AA/Ba,CAAf","names":["computed","defineCustomElement","useRoute","useStore","name","setup","store","route","shopId","params","id","cartList","state","cartTotal","productList","count","i","produnct","cartPrice","price","product"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/shop/Cart.vue"],"sourcesContent":["<template>\n  <div class=\"cart\">\n    <div class=\"check\">\n      <div class=\"check__icon\">\n        <img class=\"check__icon__img\" src=\"http://www.dell-lee.com/imgs/vue3/basket.png\">\n        <div class=\"check__icon__tag\">{{cartTotal}}</div>\n      </div>\n      <div class=\"check__info\">\n        总计: <span class=\"check__info__price\">&yen;128</span>\n      </div>\n      <div class=\"check__btn\">去结算</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed, defineCustomElement } from 'vue'\nimport { useRoute } from 'vue-router'\nimport { useStore } from 'vuex'\n\nexport default {\n  name: 'Cart',\n  setup () {\n    const store = useStore()\n    const route = useRoute()\n    const shopId = route.params.id\n    const cartList = store.state.cartList\n    // 计算购物车物品数量\n    const cartTotal = computed(() => {\n      const productList = cartList[shopId]\n      let count = 0\n      if (productList) {\n        for (const i in productList) {\n          const produnct = productList[i]\n          count += produnct.count\n        }\n      }\n      return count\n    })\n    // 计算购物车物品价格\n    const cartPrice = computed(() => {\n      const productList = cartList[shopId]\n      let price = 0\n      if (productList) {\n        for (const i in productList) {\n          const product = productList[i]\n          price = product.count * product.price\n        }\n      }\n    })\n    return { cartTotal }\n  }\n}\n</script>\n\n<style lang='scss' scoped>\n@import '../../style/viriables.scss';\n\n  .cart {\n    position: absolute;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    height: .5rem;\n    box-shadow: 0 -1px 1px 0 $content-bgColor;\n  }\n  .check {\n    display: flex;\n    height: .5rem;\n    line-height: .5rem;\n    &__icon {\n      position: relative;\n      width: .84rem;\n      line-height: .5rem;\n      &__img {\n        display: block;\n        margin: .12rem auto;\n        width: .28rem;\n        height: .26rem;\n      }\n      &__tag {\n        position: absolute;\n        padding: 0 .01rem;\n        left: .3rem;\n        right: .17rem;\n        top: .03rem;\n        min-width: .2rem;\n        height: .2rem;\n        line-height: .2rem;\n        text-align: center;\n        background: $highlight-color;\n        border-radius: .1rem;\n        transform: scale(.5);\n        color: $bgColor;\n      }\n    }\n    &__info {\n      flex: 1;\n      font-size: .12rem;\n      color: #333;\n      &__price {\n        font-size: .18rem;\n        color: $highlight-color;\n      }\n    }\n    &__btn {\n      width: .98rem;\n      text-align: center;\n      font-size: .14rem;\n      color: $bgColor;\n      background: #4FB0F9;\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}