{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport { reactive } from 'vue';\nimport post from '../../utils/request.js';\nexport default {\n  name: 'Login',\n\n  setup() {\n    const data = reactive({\n      username: '',\n      password: ''\n    }); // 路由权限跳转 islogin\n\n    const router = useRouter();\n\n    const handlelogin = async () => {\n      try {\n        const result = await post('/vue/login', {\n          username: data.username,\n          password: data.password\n        });\n\n        if (result.data.error === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: 'Home'\n          });\n        } else {\n          alert('登录失败');\n        }\n      } catch (e) {\n        alert('请求失败');\n      }\n    };\n\n    const handleRegisterClcik = () => {\n      router.push({\n        name: 'Register'\n      });\n    };\n\n    return {\n      handlelogin,\n      handleRegisterClcik,\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,OAAOC,IAAP,MAAiB,wBAAjB;AACA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,OAAI,GAAK;AACP,UAAMC,IAAG,GAAIJ,QAAQ,CAAC;AACpBK,cAAQ,EAAE,EADU;AAEpBC,cAAQ,EAAE;AAFU,KAAD,CAArB,CADO,CAKP;;AACA,UAAMC,MAAK,GAAIT,SAAS,EAAxB;;AACA,UAAMU,WAAU,GAAI,YAAY;AAC9B,UAAI;AACF,cAAMC,MAAK,GAAI,MAAMR,IAAI,CAAC,YAAD,EAAe;AACtCI,kBAAQ,EAAED,IAAI,CAACC,QADuB;AAEtCC,kBAAQ,EAAEF,IAAI,CAACE;AAFuB,SAAf,CAAzB;;AAIA,YAAIG,MAAM,CAACL,IAAP,CAAYM,KAAZ,KAAsB,CAA1B,EAA6B;AAC3BC,sBAAY,CAACC,OAAb,GAAuB,IAAvB;AACAL,gBAAM,CAACM,IAAP,CAAY;AAAEX,gBAAI,EAAE;AAAR,WAAZ;AACF,SAHA,MAGO;AACLY,eAAK,CAAC,MAAD,CAAL;AACF;AACF,OAXA,CAWE,OAAOC,CAAP,EAAU;AACVD,aAAK,CAAC,MAAD,CAAL;AACF;AACF,KAfA;;AAgBA,UAAME,mBAAkB,GAAI,MAAM;AAChCT,YAAM,CAACM,IAAP,CAAY;AAAEX,YAAI,EAAE;AAAR,OAAZ;AACF,KAFA;;AAGA,WAAO;AAAEM,iBAAF;AAAeQ,yBAAf;AAAoCZ;AAApC,KAAP;AACF;;AA7Ba,CAAf","names":["useRouter","axios","reactive","post","name","setup","data","username","password","router","handlelogin","result","error","localStorage","isLogin","push","alert","e","handleRegisterClcik"],"sourceRoot":"","sources":["/Users/mac/Desktop/jingdong/src/views/login/Login.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\">\n    <div class=\"wrapper__input\">\n      <input class=\"wrapper__input__content\" v-model=\"data.username\" placeholder=\"请输入用户名\"/>\n    </div>\n    <div class=\"wrapper__input\">\n      <input type=\"password\" class=\"wrapper__input__content\"  v-model=\"data.password\" placeholder=\"请输入密码\" />\n    </div>\n    <div class=\"wrapper__login-button\" @click='handlelogin'>登录</div>\n    <div class=\"wrapper__login-link\">\n      <span class=\"wrapper__login-link-registe\" @click='handleRegisterClcik'>立即注册</span>\n      <span class=\"wrapper__login-link-separate\">|</span>\n      <span class=\"wrapper__login-link-reset\">忘记密码</span>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRouter } from 'vue-router'\nimport axios from 'axios'\nimport { reactive } from 'vue'\nimport post from '../../utils/request.js'\nexport default {\n  name: 'Login',\n  setup () {\n    const data = reactive({\n      username: '',\n      password: ''\n    })\n    // 路由权限跳转 islogin\n    const router = useRouter()\n    const handlelogin = async () => {\n      try {\n        const result = await post('/vue/login', {\n          username: data.username,\n          password: data.password\n        })\n        if (result.data.error === 0) {\n          localStorage.isLogin = true\n          router.push({ name: 'Home' })\n        } else {\n          alert('登录失败')\n        }\n      } catch (e) {\n        alert('请求失败')\n      }\n    }\n    const handleRegisterClcik = () => {\n      router.push({ name: 'Register' })\n    }\n    return { handlelogin, handleRegisterClcik, data }\n  }\n}\n</script>\n\n<style lang='scss' scoped>\n@import '../../style/viriables.scss';\n  .wrapper {\n    position: absolute;\n    top: 50%;\n    left: 0;\n    right: 0;\n    transform: translateY(-50%);\n    &__img {\n      display: block;\n      margin:0 auto .4rem;\n      width: .66rem;\n      height: .66rem\n    }\n    &__input {\n      margin: 0 .4rem .16rem;\n      padding: 0 .16rem;\n      height: .48rem;\n      background: #F9F9F9;\n      border: 1px solid rgba(0,0,0,0.10);\n      border-radius: .06rem;;\n      &__content {\n        width: 100%;\n        line-height: .48rem;\n        border: none;\n        outline: none;\n        font-size: .16rem;\n        background: none;\n        &::placeholder {\n          color: $content-notice-fontcolor;\n        }\n      }\n    }\n    &__login-button {\n      margin: .32rem .4rem .16rem;\n      height: .48rem;\n      box-shadow: 0 .04rem .08rem 0 rgba(0,145,255,0.32);\n      border-radius: .04rem;\n      background: #0091FF;\n      text-align: center;\n      line-height: .48rem;\n      font-size: .16rem;\n      color: #fff;\n    }\n    &__login-link {\n      text-align: center;\n      font-size: .14rem;\n      color: $content-notice-fontcolor;\n      &-separate {\n        margin: 0 .12rem;\n      }\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}